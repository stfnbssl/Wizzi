module
    import 
        @ Document
        from "mongoose"
    export 
        :type IUser
            :{ 
                :p userName
                    :string 
                :p realName
                    :string 
                :p email
                    :string 
                :p hash
                    :string 
                :p salt
                    :string 
                :p createdAt
                    :ref Date
                :p lastAccess
                    :ref Date
                :m setPassword
                    :void 
                    param password
                        :string 
                :m validatePassword
                    :boolean 
                    param password
                        :string 
                :m generateJWT
                    :any 
    export 
        :interface IUserModel
            :extends IUser
            :extends Document
    export 
        :type IToken
            :{ 
                :p kind
                    :string 
                :p token
                    :string 
                :p attributes
                    :{ 
                        :index 
                            :string 
                            param k
                                :string 
    export 
        :interface ITokenModel
            :extends IToken
            :extends Document
    export 
        :type IAccount
            :{ 
                :p domain
                    :string 
                :p uid
                    :string 
                :p username
                    :string 
                :p displayName
                    :string 
                :p avatar_url
                    :string 
                :p tokens
                    :tuple 
                        :ref IToken
    export 
        :interface IAccountModel
            :extends IAccount
            :extends Document
