module

    $
        var md = {
            name: 'ToggleSwitch',
            from: '../../src/components/widgets'
        }

    v08/ts/react/stories(&md)

        $include snackcomp

        $append template
            < ToggleSwitch
                @ {...args}

        v08/ts/react/story(On)
            @ checked true
            @ label 'Toggle switch'

        v08/ts/react/story(Off)
            @ checked false
            @ label 'Toggle switch'
