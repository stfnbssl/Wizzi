$group

    +
    export
        async-function getPackageProductionObject
            param owner
                :string
            param name
                :string
            return 
                new Promise
                    => 
                        param resolve
                        param reject
                        _ getPackageProduction
                            @ owner
                            @ name
                            ._ then
                                =>
                                    param result
                                    if !result.ok
                                        return reject(result)
                                    const tf
                                        :ref IPackageProductionModel
                                        = result.item
                                    log 'myname', 'getPackageProductionObject.tf', tf
                                    const tf_packiFiles_object
                                        :ref packiTypes.PackiFiles
                                        _ JSON.parse(tf.packiFiles)
                                    log 'myname', 'getPackageProductionObject.tf_packiFiles_object', tf_packiFiles_object
                                    const obj
                                        { 
                                            @ ...tf._doc
                                            @ packiFiles tf_packiFiles_object
                                            @ _id tf._id.toString()
                                    log 'myname', 'getPackageProductionObject', obj
                                    return 
                                        _ resolve
                                            @ obj
                            catch_reject( getPackageProduction_withCache.getPackageProduction )

    +
    export
        async-function getPackageProduction_withCache
            param owner
                :string
            param name
                :string
            var cacheKey = owner + '|' + name
            log 'getPackageProduction_withCache.cacheKey', cacheKey
            return 
                new Promise
                    => 
                        param resolve
                        param reject
                        let tfValue = packageCache.get(cacheKey)
                        if tfValue
                            return resolve(tfValue)
                        _ getPackageProduction
                            @ owner
                            @ name
                            ._ then
                                =>
                                    param result
                                    if !result.ok
                                        return reject(result)
                                    const tf
                                        :ref IPackageProductionModel
                                        = result.item
                                    const tf_packiFiles_object
                                        :ref packiTypes.PackiFiles
                                        _ JSON.parse(tf.packiFiles)
                                    set tfValue
                                        { 
                                            @ packiFiles tf_packiFiles_object
                                    _ packageCache.set(cacheKey, tfValue)
                                    return resolve(tfValue)
                            catch_reject( getPackageProduction_withCache.getArtifactProduction )

    +
    export
        function invalidateCache
            param owner
                :string
            param name
                :string
            var cacheKey = owner + '|' + name
            _ packageCache.del(cacheKey)


