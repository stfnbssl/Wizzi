$group
    $params &md

    misc( setMyProps, &md )

    # see https://mxstbr.blog/2016/11/styled-components-magic-explained/
    import styled
        @ keyframes
        from 'styled-components'
    +
    $hook imports
    :interface ${md.name}Props
        $foreach prop in myProps
            $include setPropType
    +
    export
        styled ${md.name} .${md.element}<${md.name}Props>
            $hook