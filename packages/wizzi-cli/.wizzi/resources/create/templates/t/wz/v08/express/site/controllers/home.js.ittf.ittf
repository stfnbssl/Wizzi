$group

    import { Router, Request, Response } from 'express';

    export
        class HomeController
        const path = ''
        const router = Router()

        m initialize
            param initValues
            _ this.router.get(`/`, this.home)

        m home
            param request
            param response
            param next
            _ response.render
                @ 'home/index.html.ittf'
                {
                    @ title 'Site Home'