module
	kind jsfile    

    $
        var opt = {
            name: 'jsWizzi_Step_1',
            title: 'jsWizzi - step - 1 - global variables',
            folderLevel: 2
        }

    common\example\mtree(&opt)

        var ittfUri
            _ path.join
                @ __dirname
                @ 'ittf'
                $$ @ 'global_1.js.ittf'
                @ 'vars_1.js.ittf'

        var productionManager 
