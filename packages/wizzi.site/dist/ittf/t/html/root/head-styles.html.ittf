$group

    $if wzCtx.Build.useGoogleFonts
        $foreach item in wzCtx.Build.googleFonts
            css https://fonts.googleapis.com/css?family=${item}

    $if wzCtx.Build.useBootstrap
        css https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/css/bootstrap.min.css
	
	$if wzCtx.Build.useCodemirror
        css /static/lib/codemirror/lib/codemirror.css
		css /static/lib/codemirror/addon/display/fullscreen.css

    $if wzCtx.Build.useFontAwesome
        $ 
            var fontAwesomeKey = 'eab461efef';
        script
            src https://kit.fontawesome.com/${fontAwesomeKey}.js
            crossorigin anonymous

    $if wzCtx.Build.useHighlight
        $*
        link 
            rel stylesheet
            href https://cdnjs.cloudflare.com/ajax/libs/highlight.js/10.5.0/styles/default.min.css
        *$
        css /static/lib/highlightjs-master/dracula.css

	$if wzCtx.Build.usePrism
	    $if isWizziStudio
		    css /static/lib/prism/prism.css
		$else
		    css https://cdnjs.cloudflare.com/ajax/libs/prism/1.6.0/themes/prism.min.css

    $if wzCtx.Build.useReact
        css https://fonts.googleapis.com/icon?family=Material+Icons

    $if wzCtx.Build.useTailwind
		css /ittf/styles/tailwind.css

    $if wzCtx.Build.mainCss && wzCtx.Build.mainCss.length > 0
        css ${wzCtx.Build.mainCss}