        . code-hint $$ css-12vi51d
            input 
                type text
                readonly 
                aria-label Webhook endpoint
                class code-hint-input
                value ${code.value}
            button Copy
                type button
                title Copy to clipboard
                aria-label ${code.label}
                class code-hint-button