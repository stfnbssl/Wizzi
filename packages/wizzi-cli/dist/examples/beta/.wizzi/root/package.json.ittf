{
    name "${pkg.name}"
    version "${pkg.version}"
    { author
        name "${pkg.author.name}"
        email "${pkg.author.email}"
    private false
    [ licenses
        {
            type "${pkg.license.type}"
            url "${pkg.license.url}"
    main "${pkg.main}"
    { scripts
        $include v08/package/scripts
    { dependencies
        $include v08/package/dependencies
    { devDependencies
        $include v08/package/devDependencies
